{
    "manifest_version": 3,
    "name": "YouTube Playback Info",
    "version": "1.0",
    "description": "Adds info overlay to YouTube videos",
    "content_scripts": [
        {
            "matches": [
                "*://*.youtube.com/*"
            ],
            "js": [
                "jquery.js",
                "overlay.js"
            ]
        }
    ]
}
